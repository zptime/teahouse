{
  "pages": [
    "pages/index/index",
    "pages/logs/logs",
    "pages/classify/classify",
    "pages/news/news"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom"
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "tabBar": {
    "color": "#4C4D4E",
    "selectedColor": "#BDA066",
    "list": [{
        "text": "首页",
        "pagePath": "pages/index/index",
        "iconPath": "/static/images/home.png",
        "selectedIconPath": "/static/images/home-fill.png"
      },
      {
        "text": "分类",
        "pagePath": "pages/classify/classify",
        "iconPath": "/static/images/all.png",
        "selectedIconPath": "/static/images/all-fill.png"
      },
      {
        "text": "新闻",
        "pagePath": "pages/news/news",
        "iconPath": "/static/images/news.png",
        "selectedIconPath": "/static/images/news-fill.png"
      }
    ]
  },
  "usingComponents": {
    "m-header": "components/m-header/m-header"
  }
}